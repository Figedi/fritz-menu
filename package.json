    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-hot-loader": "3.0.0-beta.6",
    "react-modal": "^2.2.4",
    "react-photonkit": "^0.4.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.6",
    "react-vis": "^1.7.4",
    "recompose": "^0.25.0",
    "redux": "^3.7.1",
