{
  "name": "fritzbox-menu",
  "productName": "FritzboxMenu",
  "version": "1.0.0",
  "description": "Menubar to show current bandwidth in fritzbox",
  "scripts": {
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer\"",
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.dev.dll.js --colors",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.main.prod.js --colors",
    "build-renderer": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.prod.js --colors",
    "dev": "cross-env START_HOT=1 npm run start-renderer-dev",
    "package": "npm run build && build --publish never",
    "package-all": "npm run build && build -mwl",
    "postinstall": "concurrently \"npm run build-dll\" \"electron-builder install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./app/main.dev",
    "start-renderer-dev": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.renderer.dev.js"
  },
  "browserslist": "electron 1.6",
  "build": {
    "productName": "FritzboxMenu",
    "appId": "de.figedi.FritzboxMenu",
    "files": [
      "dist/",
      "resources/icons/",
      "node_modules/",
      "app.html",
      "main.prod.js",
      "main.prod.js.map",
      "package.json"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "directories": {
      "buildResources": "resources",
      "output": "release"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Figedi/fritz-menu.git"
  },
  "author": {
    "name": "Felix Kaminski",
    "email": "kaminski.felix@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Figedi/fritz-menu.git/issues"
  },
  "keywords": ["fritzbox", "bandwidth", "electron", "menubar"],
  "homepage": "https://github.com/Figedi/fritz-menu.git#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "chalk": "^2.0.1",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.5",
    "devtron": "^1.4.0",
    "dotenv-safe": "^4.0.4",
    "electron": "^1.7.5",
    "electron-builder": "^19.22.1",
    "electron-context-menu": "^0.9.1",
    "electron-devtools-installer": "^2.2.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-compat": "^1.0.4",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "fbjs-scripts": "^0.8.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.29.0",
    "minimist": "^1.2.0",
    "node-sass": "^4.5.3",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.1",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "url-loader": "^0.5.8",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.8.2",
