{
  "presets": [
    [
      "env",
      {
        "targets": { "node": 7 },
        "useBuiltIns": true
      }
    ],
    "stage-0",
    "react"
  ],
  "plugins": ["add-module-exports"],
  "env": {
    "production": {
      "presets": ["react-optimize"],
    },
    "development": {
      "plugins": ["transform-class-properties", "transform-es2015-classes"]
    }
  }
}
